#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules/coffee-script/bin/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules/coffee-script/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules/coffee-script/bin/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules/coffee-script/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/coffee-script@1.7.1/node_modules:/Users/robert.mason/IdeaProjects/tongue-drum-chum/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/coffee" "$@"
else
  exec node  "$basedir/../../bin/coffee" "$@"
fi
